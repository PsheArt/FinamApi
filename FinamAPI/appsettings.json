{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FinamApiSettings": {
    "BaseUrl": "http://api.finam.ru",
    "UserAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36 Edg/91.0.864.59.",
    "RateLimits": {
      "Securities": 1,
      "Portfolio": 100,
      "Orders": 100,
      "Stops": 100,
      "Candles": 120,
      "TimeWindowMinutes": 1
    },

    "HttpClientSettings": {
      "TimeoutSeconds": 30,
      "MaxRetryAttempts": 3,
      "RetryDelayMilliseconds": 1000
    }
  }

}
